<?xml version="1.0"?>
<module name="@PACKAGE@" version="@VERSION@" release="@RELEASE@">

    <description>jQuery is a fast and concise JavaScript Library that simplifies HTML document traversing, event handling, animating, and Ajax interactions for rapid web development.</description>

    <requires>
      <module name="dynacase-jquery-1.7.2-installer" />
    </requires>

    <post-install>
        <process command="programs/jquery_default @VERSION@"/>
    </post-install>

    <post-upgrade>
        <process command="programs/jquery_default @VERSION@"/>
    </post-upgrade>

</module>
